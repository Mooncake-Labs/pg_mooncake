{
    "build": {
        "dockerfile": "Dockerfile_17"
    },
    "postStartCommand": "bash .devcontainer/postStartCommand.sh",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-vscode.cpptools",
                "eamodio.gitlens",
                "rust-lang.rust-analyzer"
            ]
        }
    },
    "runArgs": [
        "--cap-add=SYS_PTRACE"
    ],
    "features": {
        "ghcr.io/devcontainers/features/sshd:1": {
            "version": "latest"
        }
    }
}
